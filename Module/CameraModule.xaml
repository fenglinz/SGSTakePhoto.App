<UserControl x:Class="SGSTakePhoto.App.CameraModule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SGSTakePhoto.App"
             xmlns:wpfmedia="clr-namespace:WPFMediaKit.DirectShow.Controls;assembly=WPFMediaKit"
             mc:Ignorable="d"
             Loaded="UserControl_Loaded" Name="Camera"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <wpfmedia:VideoCaptureElement Name="VideoCapture"/>
    </Grid>
</UserControl>
